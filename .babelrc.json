{
  "presets": [["module:metro-react-native-babel-preset"]],
  "plugins": [
    "module:react-native-dotenv",
    "jotai/babel/plugin-react-refresh",
    [
      "@babel/plugin-transform-react-jsx",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": ["transform-remove-console"]
    }
  }
}
